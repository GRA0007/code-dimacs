{
  "name": "code-dimacs",
  "displayName": "DIMACS",
  "description": "Language support for CNF/DNF files in DIMACS format",
  "version": "0.0.2",
  "publisher": "giraugh",
  "repository": "https://github.com/giraugh/code-dimacs",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "dimacs",
      "aliases": ["DIMACS", "cnf", "dnf"],
      "extensions": [".cnf", ".dnf"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "dimacs",
      "scopeName": "source.dimacs",
      "path": "./syntaxes/dimacs.tmLanguage.json"
    }]
  }
}
